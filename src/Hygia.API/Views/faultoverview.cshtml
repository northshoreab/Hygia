<section id="faultoverview-view" class="view">
    <div class="row">
        <div class="span2" data-bind="template: {name: faultSummaryTemplate}"></div>
        <div class="span5">
            <div id="wijbarchart" data-bind="wijbarchart: {seriesList: faultsLastWeek}" style="height: 300px"></div>
            
        </div>
        <div class="span5">
            <div id="wijlinechart" data-bind="wijlinechart: {seriesList: faultsLastWeek}" style="height: 300px;"></div>            
        </div>
    </div>
    <div class="row">
        <div class="span12">
            <div data-bind="template: {name: 'faults.view', foreach: visibleFaults}"></div>
            <ul class="pager">
                <li class="previous" data-bind="css: {disabled: !canMovePrevious()}"><a href="#" data-bind="click: previousPage">Previous</a></li>
                <li class="next" data-bind="css: {disabled: !canMoveNext()}"><a href="#" data-bind="click: nextPage">Next</a></li>
            </ul>
        </div>
    </div>
</section>